
#include <iostream>

using namespace std;

int main()
{
    int max=0;
 string maximam;
    int T;
    1<=T<=100;
    cin>>T;

    for(int i=0; i<T; i++){
        int t;
        2<=t<=100;
        cin>>t;
        int arr[t];
        string city[t];

        for(int j=0; j<t; j++){
            cin>>city[j];
            cin>>arr[j];

            if(arr[j]>max)
               {
                max=arr[j];
                maximam+=city[j];


            }


        }

   }

        cout<<maximam;





    return 0;
}
